// gives tle on 46th case optimize this 

class Solution {
public:
    vector<vector<int>> dp;
    int fun(int i,int j,string s1,string s2){
        if(i<0 || j<0) return 0;

        if(dp[i][j]!=-1){
            return dp[i][j];
        }
        //agar match hua toh
        if(s1[i]==s2[j]){
            return 1+fun(i-1,j-1,s1,s2);
        }
        // agar nhi hua toh
        return dp[i][j]=max(fun(i-1,j,s1,s2),fun(i,j-1,s1,s2));
    }
    int longestCommonSubsequence(string s1, string s2) {
        int n=s1.length(),m=s2.length();
        dp.assign(n,vector<int> (m,-1));
        return fun(n-1,m-1,s1,s2);
    }
};