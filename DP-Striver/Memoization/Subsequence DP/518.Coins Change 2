    int change(int amount, vector<int>& coins) {
        int n = coins.size();
        dp.assign(n, vector<long long>(amount+1, 0));
        for(int i=0;i<=amount;i++){
            if(i%coins[0]==0){
                dp[0][i]=1;
            }
        }
        for(int i=0;i<n;i++){
            dp[i][0]=1;
        }
        for(int i=1;i<n;i++){
            for(int j=0;j<=amount;j++){
                int not_take=dp[i-1][j];
                int take=0;
                if(j-coins[i]>=0){
                    take=dp[i][j-coins[i]];
                }
                dp[i][j]=(long long)not_take+take;
            }
        }
        return (int)dp[n-1][amount];