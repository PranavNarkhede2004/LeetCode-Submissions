class Solution {
public:
    vector<vector<int>> dp;
    int fun(vector<vector<int>>& grid,int m,int n){
        if(m<0 || n<0) return 0;
        if(grid[m][n]==1) return 0;
        if(m==0 && n==0) return 1;
        if(grid[m][n]==0){
            // we can go here
            if(dp[m][n]==-1){
                return dp[m][n]=fun(grid,m-1,n)+fun(grid,m,n-1);
            }else return dp[m][n];
            
        }else return 0;
    }
    int uniquePathsWithObstacles(vector<vector<int>>& grid) {
        int m=grid.size();
        int n=grid[0].size();
        dp.assign(m,vector<int>(n,-1));
        return fun(grid,m-1,n-1);
    }
};