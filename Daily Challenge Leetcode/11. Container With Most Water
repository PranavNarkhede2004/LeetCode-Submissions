class Solution {
public:
    int maxArea(vector<int>& height) {
        int n=height.size();
        int left=0,right=n-1;
        int maxi=-1;
        while(left<right){
            maxi=max(maxi,min(height[left],height[right])*(right-left));
            if(height[left]>height[right]){
                //move right pointer
                right--;
            }else left++;
        }
        return maxi;
    }
};