class Solution {
public:
    static bool cmp(const vector<int> &a,const vector<int> &b){
        return a[1]<b[1];
    }
    int eraseOverlapIntervals(vector<vector<int>>& A) {
        int n=A.size();
        sort(A.begin(),A.end(),cmp);
        int end=A[0][1];
        int cnt=1;
        for(int i=1;i<n;i++){
            if(A[i][0]>=end){
                cnt++;
                end=A[i][1];
            }
        }
        
        return n-cnt;
    }
};