class Solution {
public:
    int jump(vector<int>& nums) {

        int n=nums.size();
        if(n==1) return 0;
        int maxInd=-1;
        int cnt=0;
        int r=0,l=0;

       while(r<n-1){
        int farthest=0;
        for(int i=l;i<=r;i++){
            farthest=max(farthest,i+nums[i]);
        }
        l=r+1;
        cnt++;
        r=farthest;
       }
        return cnt;
    }
};